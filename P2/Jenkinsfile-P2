pipeline {
    agent any
    environment {
    JOBNAME = "testing-PR"
    BUILD_NUMBER = "19"
    }
    stages {
        stage('Git') {
            steps {
                step([$class: 'WsCleanup'])
                checkout scm
            }
        }

        stage('Making Project2') {
            steps {
                echo 'Creating..'
               // Make the output directory.
                sh "mkdir -p output"
                sh "cd P2 && ls -l"
                sh "pwd"
                sh "cd .. && pwd"
                }
        }

        stage('pull artifact') {
            steps {
                copyArtifacts filter: 'test.zip', fingerprintArtifacts: true, projectName: 'testing-PR', selector: specific('19')
                sh 'unzip test.zip'
                sh 'cat archive/test.txt' 
            }
        }

    }
}
