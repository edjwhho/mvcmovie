                sh "cd P2 && ls -l"
                sh "pwd"
                sh "cd .. && pwd"
                sh "echo ${buildNumber} ${PROJNAME}"
                sh "printenv"
            }
        }
                }
        }

        stage('pull artifact') {
            steps {
                copyArtifacts(
                    filter: 'test.zip',
                    projectName: 'testing-PR',
                    fingerprintArtifacts: true,
                    selector: specific('19')
                )
                sh 'unzip test.zip'
                sh 'cat archive/test.txt' 
            }
        }

    }
}